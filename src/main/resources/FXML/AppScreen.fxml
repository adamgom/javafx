<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" spacing="20.0">
         <children>
            <Label fx:id="label" text="Wypisz dane" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <TextArea fx:id="textArea" maxWidth="300.0" prefHeight="50.0" />
            <Slider fx:id="slider" max="10.0" prefWidth="450.0" showTickMarks="true" />
            <HBox alignment="CENTER" spacing="15.0">
               <children>
                  <Button fx:id="addButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="Dodaj" />
                  <Button fx:id="removeButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="Usuń" />
                  <Button fx:id="printButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="Wyświetl" />
                  <Button fx:id="saveButton" mnemonicParsing="false" text="Zapisz" />
               </children>
            </HBox>
            <Separator />
            <HBox alignment="CENTER" spacing="100.0">
               <children>
                  <VBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Label fx:id="auxLabel1" prefWidth="50.0" text="Label" />
                        <Label fx:id="auxLabel2" prefWidth="50.0" text="Label" />
                     </children>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </VBox>
                  <Button fx:id="returnToMenuButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="Wstecz" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</Pane>
